{
	"name": "orisai/cmf-project",
	"description": "Orisai CMF project skeleton",
	"keywords": [
		"orisai",
		"project",
		"skeleton"
	],
	"type": "project",
	"license": "Unlicense",
	"require": {
		"php": ">=8.0.0 <8.2.0",
		"orisai/cmf-core": "^1.0.0",
		"orisai/cmf-ui": "^1.0.0",
		"symfony/dotenv": "^5.2.0"
	},
	"require-dev": {
		"infection/infection": "^0.22.1",
		"nextras/orm-phpstan": "^0.6.2",
		"orisai/coding-standard": "^2.0.0",
		"phpstan/extension-installer": "^1.0.0",
		"phpstan/phpstan": "^0.12.0",
		"phpstan/phpstan-deprecation-rules": "^0.12.0",
		"phpstan/phpstan-nette": "^0.12.0",
		"phpstan/phpstan-phpunit": "^0.12.0",
		"phpstan/phpstan-strict-rules": "^0.12.0",
		"phpunit/phpunit": "^9.3.0",
		"staabm/annotate-pull-request-from-checkstyle": "^1.1.0",
		"symfony/thanks": "^1.0.0"
	},
	"autoload": {
		"psr-4": {
			"App\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\App\\": "tests"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true,
		"platform": {
			"php": "8.0"
		}
	}
}
